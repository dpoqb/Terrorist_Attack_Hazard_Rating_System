<!--test: reference OGNL-->
    <select id="getSearchEvent" resultMap="BaseResultMap">
        SELECT * FROM tevent_all_info
        <where>
            <if test="eventid!=null and eventid!=''">
                eventid = #{eventid}
            </if>
            <if test="IDX!=null and IDX!=0">
                IDX = #{IDX}
            </if>
            <if test="iyear!=-1">
                AND iyear = #{iyear}
            </if>
            <if test="imonth!=-1">
                AND imonth = #{imonth}
            </if>
            <if test="iday!=-1">
                AND iday = #{iday}
            </if>
            <!--the number of people killed-->
            <if test="nkillMin==0 and nkillMax==0">
                AND (nkill >= #{nkillMin} OR nkill = NULL)
            </if>
            <if test="nkillMin!=0 or nkillMax!=0">
                AND nkill BETWEEN #{nkillMin} AND #{nkillMax}
            </if>
            <!--region-->
            <if test="region_txt!=&quot;null&quot; and region_txt!=''">
                AND region_txt LIKE #{region_txt}
            </if>
            <!--country-->
            <if test="country_txt!=&quot;null&quot; and country_txt!=''">
                AND country_txt LIKE #{country_txt}
            </if>
            <if test="targtype!=&quot;null&quot; and iday!=''">
                AND (targtype1_txt LIKE #{targtype} OR
                targtype2_txt LIKE #{targtype} OR
                targtype3_txt LIKE #{targtype})
            </if>
            <if test="attacktype!=&quot;null&quot; and iday!=''">
                AND (attacktype1_txt LIKE #{attacktype} OR
                attacktype2_txt LIKE #{attacktype} OR
                attacktype3_txt LIKE #{attacktype})
            </if>
            <if test="gname!=&quot;null&quot; and iday!=''">
                AND (gname LIKE #{gname} OR
                gname2 LIKE #{gname} OR
                gname3 LIKE #{gname})
            </if>
        </where>
    </select>

    <delete id="deleteByPrimaryKey">
        DELETE FROM tevent_all_info
        WHERE eventid = #{eventid}
    </delete>

