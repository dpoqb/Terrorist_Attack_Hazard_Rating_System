    <!--id tied to abstract method of interface
    public TerrorEvent getEventId(Integer id);-->
    <select id="getDefaultEvent" resultMap="BaseResultMap">
        SELECT *
        FROM tevent_all_info
        ORDER BY IDX DESC
        LIMIT 0,50
    </select>
<!--search update event-->
    <select id="selectByPrimaryKey" resultMap="BaseResultMap">
        SELECT *
        FROM tevent_all_info
        WHERE eventid = #{eventid}
    </select>

    <!--count event-->

